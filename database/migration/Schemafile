create_table "restaurants", force: :cascade do |t|
  t.string   "name"
  t.string     "address"
  t.string     "tellnumber"
  t.string "opening_hours"
  t.datetime "created_at"
  t.datetime "updated_at"
end

create_table "articles", force: :cascade do |t|
  t.string   "title"
  t.text     "desc"
  t.text     "author"
  t.datetime "created_at"
  t.datetime "updated_at"
end

